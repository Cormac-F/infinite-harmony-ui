        "outDir": "./dist/",
        "target": "ES5",
        "sourceMap": true,
        "noImplicitAny": true,
        "module": "commonjs",
        "moduleResolution": "node",
    },
    "types": ["node", "dotenv/config"],
    "include": ["./**/*"],
    "exclude": [
        "node_modules/**/*",
        "./dist/**/*"
    ]
}